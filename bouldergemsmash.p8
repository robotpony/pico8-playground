-- hud and popups

function popup_draw()
	xs=m.pxx+msg.x+6
	xe=m.pxx+msg.x+msg.w
	ys=m.pxy+msg.y+6
	ye=m.pxy+msg.y+msg.h
	
	rect(xs+1,ys+1,xe+1,ye+1,1)
	rectfill(xs,ys,xe,ye,0)
	rect(xs,ys,xe,ye,msg.c)
	print(msg.text,xs+4,ys+4,msg.c)
end

function hud_draw()
 bx=0+m.pxx
 by=120+m.pxy
 
 rectfill(bx,by,bx+128,by+8,6)
 line(bx,by+7,bx+127,by+7,5)
 line(bx,by,bx+127,by,7)
 
 if (debug==1) then
  p.keys=200
 	print('p'..p.x..','..p.y,bx+1,by+1)
 	print('m'..m.x..','..m.y,bx+32,by+1)
 	return
 end
 
 print('♥'..p.hp,bx+1,by+1,1)
 
 keys='🅾️'..p.keys
 klen=#keys - 1
 center=64-((klen/2)*6)-6
 print(keys,center,by+1,1)
 
 score='★'..p.score
 slen=#score
 ralign=(bx+128)-(slen*6)
 print(score,ralign,by+1,1)
end

function explain(text,x,y,tc)
	p.hints+=1
	if (t.mode==1) then
		
		if (p.hints>10) then
			-- end tutorial			
			t.mode=3
			popup('you\'re on\nyour own',x,y,9)
		else
					popup(text,x,y,tc)
		end
	end
	
end


function popup(text,x,y,tc)
	pause()
	msg.text=text
	
	if (x<9) then 
		msg.x=(x*8)+4
	else 
		msg.x=x
	end
	
	if (y<9) then 
		msg.y=(y*8)+4
	else
		msg.y=y
	end
	
	if (tc==nil) then 
		msg.c=7
	else 
		msg.c=tc
	end
end

