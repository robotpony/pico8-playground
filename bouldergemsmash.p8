__lua__
-- boulder gem smash

function _init()
 t={}
 t.now=time()
 t.last=t.now
	t.tick=0
	t.pause=0
	-- 0 title, 1 tut, 2 game, 3 gamex, 4 end
	t.mode=1
	
	debug=0
	
	msg={}
	msg.text=''
	msg.x=20
	msg.y=20
	msg.c=7
	msg.w=48
	msg.h=24
	
	map_init()
	player_init()
end

function _update()
	t.now=time()
	if(t.now>(t.last+1)) then
		t.tick=(t.tick+1)%2
		t.last=t.now
	end
	
	if (paused()) then
		player_wait()		
	else
		player_move()
	end
	
end

function _draw()
	cls()
	map_draw()
	player_draw()
	hud_draw()
		
	if (paused()) then
		popup_draw()
	end
	
end

function paused()
	return t.pause==1
end

function pause()
	t.pause=1
end

function unpause()
	t.pause=0
end



-->8
-- map

function map_init()
 -- tile bits
	solid=0
	lock=1
	falls=2
	triggers=3
	destroys=4
	no_dig=5
	hpinc=6
	hpdec=7
	
	-- 
	level=0
	tutorial=1
	
	m={}
	m.x=0
	m.y=0
	m.pxx=0
	m.pxy=0
	
	setup_map()
		
end

function setup_map()
	-- cached tile [l]ocations
	l={}
	l.sx=1
	l.sy=1
	l.ex=126
	l.ey=126

	for x=1,128 do
		for y=1,128 do
			mt=mget(x,y)
			
			if (mt==16) then
				l.sx=x
				l.sy=y
			elseif (mt==32) then
				l.ex=x
				l.ey=y
			end
			
		end
	end
	?'done'
end

function map_draw()
 m.x=flr(p.x/16)*16
 m.y=flr(p.y/15)*15
	m.pxx=m.x*8
	m.pxy=m.y*8
 camera(m.pxx,m.pxy)
 map(0,0,0,0,128,128)
end

function is_tile(tile_t,x,y)
	tile=mget(x,y)
	return fget(tile,tile_t)
end

function can_move_to(x,y)
	return not is_tile(solid,x,y)
end

function can_activate(x,y)
	return is_tile(triggers,x,y)
end

function can_unlock(x,y)
	return is_tile(lock,x,y)
end

function is_diggable(x,y)
	if (is_tile(no_dig,x,y)) return
	return mget(x,y)
end

function toggle_tile(x,y,to)
	tile=mget(x,y)

	if (to == nil) then
		toggle_to=tile+1
	else 
		toggle_to=to
	end

	mset(x,y,toggle_to)

	return toggle_to
end
-->8
-- player

function player_init()

	p={}
	--
	p.x=l.sx
	p.y=l.sy
	-- r,l,d,u
	p.sprites={1,17,33,49}
	p.sprite=p.sprites[1]
	
	p.hp=3
	p.keys=0
	p.score=0
	p.hints=0
end

function player_draw()
	spr(p.sprite+t.tick,p.x*8,p.y*8)
end

function player_wait()
	if (any_btn()) then
		unpause()
	end
end

function player_move()
	dx=0
	dy=0
	
	if (btnp(⬅️)) then dx=-1 
	elseif (btnp(➡️)) then dx=1
	elseif (btnp(⬆️)) then dy=-1
	elseif (btnp(⬇️)) then dy=1
	else
	 set_p_dir(dx,dy) 
		return 
	end
	
	nx=p.x+dx
	ny=p.y+dy
	
	set_p_dir(dx,dy)

	if (can_move_to(nx,ny)) then
		player_move_to(nx,ny)

	 if (t.tick) sfx(2)

		player_dig(nx,ny)
				 
	elseif (can_activate(nx,ny)) then
		player_trigger(nx,ny)
	else
	 sfx(1)
	end
end

function player_move_to(x,y)
	 p.x=mid(0,x,127)
	 p.y=mid(0,y,127)
end

function player_dig(x,y)

	tile = is_diggable(x,y)
 if (tile==nil) return false

	mset(x,y,0)
	sfx(2)
	p.score+=1
end

function player_trigger(x,y)
 tile=mget(x,y)
 chest=46
 key1=12
 key2=28
 hp1=11
 hp2=27
 tnt=62

	if (tile==chest) then
		p.keys+=1
		toggle_tile(x,y)
		sfx(0)
		explain('keys are\nin chests',x,y)
	elseif (tile==key1 or tile==key2) then
		p.keys+=1
		p.score+=10
		player_dig(x,y)	
		sfx(0)
		explain('keys open\ndoors',x,y)
		return true
	elseif (tile==hp1 or tile==hp2) then
		p.hp+=1
		p.score+=10
		sfx(0)
		player_dig(x,y)
		explain('♥s give\nu health',x,y)
		return true
	elseif (can_unlock(x,y) and p.keys > 0) then
		p.keys=flr(p.keys-1)
		sfx(0)
		toggle_tile(x,y,0)
		explain('the door\nis open',x,y)
	elseif (tile==tnt) then
		explain('be very\ncareful',x,y,8)
	else
		sfx(2)
	end
	
	return false
end

function set_p_dir(nx,ny)
	if (nx==1) p.sprite=p.sprites[1]
	if (nx==-1) p.sprite=p.sprites[2]
	if (ny==1) p.sprite=p.sprites[3]
	if (ny==-1) p.sprite=p.sprites[4]
end

function any_btn()
 return btnp()>0
end
